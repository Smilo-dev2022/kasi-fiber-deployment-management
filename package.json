{
  "name": "kasi-fiber-deployment-management",
  "version": "1.0.0",
  "description": "FIBER PON Tracker App - Project management for FiberTime sites",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "test": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build"
  },
  "keywords": [
    "fiber",
    "pon",
    "tracking",
    "project-management",
    "deployment"
  ],
  "author": "Kasi Fiber Team",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^10.11.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exifr": "^7.1.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.4",
    "pino-http": "^10.5.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "jest": "^30.1.3",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4"
  }
}
