# Production .env template
APP_ENV=production
PORT=8000
DATABASE_URL=${SECRET_PROD_DATABASE_URL}
JWT_SECRET=${SECRET_PROD_JWT}
JWT_ALG=HS256
WEBHOOK_HMAC_LIBRENMS=${SECRET_PROD_HMAC_LIBRENMS}
WEBHOOK_HMAC_ZABBIX=${SECRET_PROD_HMAC_ZABBIX}
NMS_ALLOW_IPS=10.0.0.0/8,192.168.0.0/16
S3_ENDPOINT=https://s3.yourdomain.co.za

S3_REGION=af-south-1
S3_BUCKET=fiber-prod
S3_ACCESS_KEY=${SECRET_PROD_S3_ACCESS_KEY}
S3_SECRET_KEY=${SECRET_PROD_S3_SECRET_KEY}
CORS_ALLOWED_ORIGINS=https://app.yourdomain.co.za,https://admin.yourdomain.co.za

TIMEZONE=Africa/Johannesburg
SENTRY_DSN=${SECRET_PROD_SENTRY_DSN}
SMTP_HOST=${SECRET_PROD_SMTP_HOST}
SMTP_PORT=587
SMTP_USER=${SECRET_PROD_SMTP_USER}
SMTP_PASS=${SECRET_PROD_SMTP_PASS}
EMAIL_FROM=noreply@yourdomain.co.za

OIDC_ISSUER_URL=${SECRET_PROD_OIDC_ISSUER}
OIDC_CLIENT_ID=${SECRET_PROD_OIDC_CLIENT_ID}
OIDC_CLIENT_SECRET=${SECRET_PROD_OIDC_CLIENT_SECRET}
